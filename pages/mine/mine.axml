<view style="margin-top:20px;">
  <list>
			<block a:for="{{itemsThumb}}">
				<list-item
					thumb="{{item.thumb}}"
					arrow="{{item.arrow}}"
					onClick="onItemClick"
					index="{{index}}"
					last="{{index === (itemsThumb.length - 1)}}"
					key="{{index}}"
				>
					{{item.title}}
				</list-item>
			</block>
		</list>
</view>
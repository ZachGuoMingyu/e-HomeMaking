<view class="header">
  <view class="icon">
    <image mode="scaleToFill" src="../../../images/icon.png"/>
  </view>
  <view class="info">
    你好，150xxxx8888
  </view>
  <view class="acount">
    <view class="yue">
      <view class="yue1">
        账户余额
      </view>
      <view class="yue2">
        ￥888.00
      </view>
    </view>
    <view class="line"></view>
